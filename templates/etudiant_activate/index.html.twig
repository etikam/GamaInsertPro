                <td>{{ etudiant_not_activate.adresse }}</td>
                <td>{{ etudiant_not_activate.encours ? 'Yes' : 'No' }}</td>
                <td>{{ etudiant_not_activate.niveau }}</td>
                <td>{{ etudiant_not_activate.matricule }}</td>
                <td>
                    <a href="{{ path('app_etudiant_activate_show', {'id': etudiant_not_activate.id}) }}">show</a>
                    <a href="{{ path('app_etudiant_activate_edit', {'id': etudiant_not_activate.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
