{% extends 'base.html.twig' %}

{% block title %}EtudiantNotActivate index{% endblock %}

{% block body %}
    <h1>EtudiantNotActivate index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Genre</th>
                <th>Handicape</th>
                <th>DateNaissance</th>
                <th>PaysResidence</th>
                <th>Adresse</th>
                <th>Encours</th>
                <th>Niveau</th>
                <th>Matricule</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etudiant_not_activate in etudiant_not_activates %}
